
<%- include('header'); -%>
  <div class="box" id="heading">
    <h1><%= thisDay %></h1>
  </div>
    <div class="box">
        

    <form class="item" method="post" action="/">
        <input name="newItem" type="text" placeholder="New item..." autocomplete="off"/>
        <button name="submit" type="submit"/>+</button>
    </form>

    <ul>
       <% for (let i = 0; i < newListItem.length; i++) { %>
        <div class="item">
          <input type="checkbox"/>
        <p><%= newListItem[i] %></p>
        </div>
        
      <% } %>
    </ul>
    </div>
   
    <%- include("footer") -%>
